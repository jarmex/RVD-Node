{
  "name": "rvdnode",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "DEBUG=ussd:* node ./dist/app.js",
    "dev": "nodemon",
    "test": "mocha --require @babel/register",
    "deploy": "./compile && ./deploy.sh yarn"
  },
  "dependencies": {
    "apollo-server-express": "2.9.15",
    "axios": "0.21.1",
    "cors": "2.8.5",
    "debug": "4.1.1",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "graphql": "14.5.8",
    "graphql-date": "1.0.3",
    "helmet": "3.21.2",
    "http-errors": "1.7.3",
    "ioredis": "4.14.1",
    "jsonpath": "1.0.2",
    "jsonwebtoken": "8.5.1",
    "merge-graphql-schemas": "1.7.5",
    "morgan": "1.9.1",
    "mysql2": "2.1.0",
    "sequelize": "5.21.3",
    "sequelize-cli": "5.5.1",
    "sqlite3": "4.1.1"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "12.3"
          },
          "loose": true
        }
      ]
    ],
    "plugins": [
      [
        "@babel/proposal-object-rest-spread",
        {
          "useBuiltIns": true
        }
      ],
      [
        "@babel/plugin-proposal-class-properties"
      ],
      [
        "@babel/plugin-transform-classes",
        {
          "loose": true
        }
      ]
    ],
    "ignore": [
      "./node_modules"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/plugin-transform-classes": "^7.7.4",
    "@babel/preset-env": "^7.7.7",
    "babel-eslint": "^10.0.3",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "mocha": "^6.2.2",
    "nodemon": "^2.0.2"
  }
}
